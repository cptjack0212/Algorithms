function mutation(arr) {
var arr0 = arr[0].toLowerCase();
var arr1 = arr[1].toLowerCase();

for (i = 0; i < arr1.length; i ++){

  var checking = arr0.indexOf(arr1[i]);
  if (checking == -1){
    return false;
  }
}
  return true;
}

mutation(["hello", "hey"]);
mutation(["Alien", "line"]);
mutation(["zyxwvutsrqponmlkjihgfedcba", "qrstu"]);
mutation(["hello", "neo"]);
